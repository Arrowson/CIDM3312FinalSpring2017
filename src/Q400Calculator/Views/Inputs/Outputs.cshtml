            </th>
            <th>
                @Html.DisplayNameFor(model => model.Precipitation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RunwayDirection)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Temperature)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TripDistance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Weight)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WindDegrees)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WindSpeed)
            </th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td>
                @Html.DisplayFor(model => model.Altitude)
            </td>
            <td>
                @Html.DisplayFor(model => model.Icing)
            </td>
            <td>
                @Html.DisplayFor(model => model.Name)
            </td>
            <td>
